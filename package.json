{
  "name": "multitenant-sample-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "better-npm-run": "^0.1.1",
    "i": "^0.3.7",
    "npm": "^8.7.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:cl": "better-npm-run start:cl",
    "start:pe": "better-npm-run start:pe",
    "build:cl": "better-npm-run build:cl",
    "build:pe": "better-npm-run build:pe",
    "test:cl": "better-npm-run test:cl",
    "test:pe": "better-npm-run test:pe",
    "eject": "react-scripts eject"
  },
  "betterScripts": {
    "start:cl": {
      "command": "react-scripts start",
      "env": {
        "REACT_APP_NAME": "Multitenant-dev-Chille",
        "REACT_APP_TENANT": "cl"
      }
    },
    "build:cl": {
      "command": "react-scripts build",
      "env": {
        "REACT_APP_NAME": "Multitenant-dev-Chille",
        "REACT_APP_TENANT": "cl"
      }
    },
    "test:cl": {
      "command": "react-scripts test",
      "env": {
        "REACT_APP_NAME": "Multitenant-dev-Chille",
        "REACT_APP_TENANT": "cl"
      }
    },
    "start:pe": {
      "command": "react-scripts start",
      "env": {
        "REACT_APP_NAME": "Multitenant-dev-Peru",
        "REACT_APP_TENANT": "pe"
      }
    },
    "build:pe": {
      "command": "react-scripts build",
      "env": {
        "REACT_APP_NAME": "Multitenant-dev-Peru",
        "REACT_APP_TENANT": "pe"
      }
    },
    "test:pe": {
      "command": "react-scripts test",
      "env": {
        "REACT_APP_NAME": "Multitenant-dev-Peru",
        "REACT_APP_TENANT": "pe"
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
